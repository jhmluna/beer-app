<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th></th>
      <th>Ticket Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.date %></td>
        <td></td>
        <td><%= order.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
